{
  "name": "Smart Filler Simple",
  "version": "2.0.1",
  "manifest_version": 3,
  "description": "Auto-fill form fields with random data",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "https://random-word-api.vercel.app/",
    "https://generativelanguage.googleapis.com/",
    "https://api.ferdev.my.id/",
    "https://www.sankavollerei.com/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "utils.js",
        "vendor/katex/katex.min.js",
        "latex.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "vendor/sweetalert2/sweetalert2.min.css",
        "vendor/sweetalert2/sweetalert2.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "vendor/katex/*",
        "vendor/katex/fonts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}